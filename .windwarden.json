{
  "sortOrder": "custom",
  "customOrder": ["effects", "transitions", "transforms", "backgrounds", "typography", "borders", "spacing", "sizing", "flexbox-grid", "layout"],
  "presetRegex": "all",
  "functionNames": ["myMerge", "cx"],
  "customRegex": [],
  "removeNullClasses": true,
  "preserveDuplicates": false,
  "ignorePaths": [
    "node_modules",
    "dist",
    "build",
    "coverage",
    ".git",
    ".next",
    ".nuxt",
    "target"
  ],
  "fileExtensions": [
    "tsx",
    "jsx",
    "ts",
    "js",
    "vue",
    "svelte"
  ],
  "categories": {},
